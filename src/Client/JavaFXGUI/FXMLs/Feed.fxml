<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="521.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.JavaFXGUI.Controllers.FeedController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="303.0" prefHeight="194.0" prefWidth="129.0" spacing="20.0">
         <children>
            <Button fx:id="followSomeoneBtn" alignment="CENTER" mnemonicParsing="false" onAction="#followSomeoneBtnClick" prefHeight="50.0" prefWidth="120.0" text="Follow Someone" />
            <Button fx:id="submitTweetBtn" alignment="CENTER" layoutX="34.0" layoutY="10.0" mnemonicParsing="false" onAction="#submitTweetBtnClick" prefHeight="50.0" prefWidth="120.0" text="Submit Tweet" />
            <Button fx:id="logOutBtn" alignment="CENTER" layoutX="34.0" layoutY="35.0" mnemonicParsing="false" onAction="#logOutBtnClick" prefHeight="50.0" prefWidth="120.0" text="Log Out" />
         </children>
      </VBox>
      <Label contentDisplay="CENTER" layoutX="311.0" layoutY="14.0" prefHeight="50.0" prefWidth="210.0" text="Welcome to Twitter 2" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="updateTweetsBtn" layoutX="723.0" layoutY="52.0" mnemonicParsing="false" onAction="#updateTweetsBtnClick" text="Update Tweets" />
      <Label layoutX="20.0" layoutY="98.0" text="Logged in as:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label id="loggedAsLabel" fx:id="loggedAsLabel" layoutX="20.0" layoutY="128.0" prefHeight="17.0" prefWidth="118.0" text="null">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ScrollPane fx:id="tweetsScrollPane" hbarPolicy="NEVER" layoutX="190.0" layoutY="89.0" prefHeight="408.0" prefWidth="628.0">
         <content>
            <VBox fx:id="tweetVBox" alignment="TOP_RIGHT" prefHeight="398.0" prefWidth="625.0" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
