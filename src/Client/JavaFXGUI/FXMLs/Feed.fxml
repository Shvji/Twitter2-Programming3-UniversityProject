<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="521.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="303.0" prefHeight="194.0" prefWidth="129.0" spacing="20.0">
         <children>
            <Button fx:id="btn1" alignment="CENTER" mnemonicParsing="false" onAction="#btn1Click" prefHeight="50.0" prefWidth="120.0" text="btn1" />
            <Button fx:id="btn2" alignment="CENTER" layoutX="34.0" layoutY="10.0" mnemonicParsing="false" onAction="#btn2Click" prefHeight="50.0" prefWidth="120.0" text="btn2" />
            <Button fx:id="btn3" alignment="CENTER" layoutX="34.0" layoutY="35.0" mnemonicParsing="false" onAction="#btn3Click" prefHeight="50.0" prefWidth="120.0" text="btn3" />
         </children>
      </VBox>
      <Label fx:id="title" contentDisplay="CENTER" layoutX="311.0" layoutY="14.0" prefHeight="50.0" prefWidth="210.0" text="Welcome to Twitter 2" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="updateTweetsBtn" layoutX="723.0" layoutY="52.0" mnemonicParsing="false" onAction="#updateTweetsBtnClick" text="Update Tweets" />
      <Label layoutX="14.0" layoutY="51.0" text="Logged in as:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label id="loggedAsLabel" fx:id="loggedAsLabel" layoutX="143.0" layoutY="55.0" prefHeight="17.0" prefWidth="118.0" text="null">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ScrollPane fx:id="tweetsScrollPane" hbarPolicy="NEVER" layoutX="190.0" layoutY="89.0" prefHeight="408.0" prefWidth="628.0">
         <content>
            <VBox fx:id="tweetVBox" alignment="TOP_RIGHT" prefHeight="398.0" prefWidth="625.0" />
         </content>
      </ScrollPane>
      <TextField fx:id="hashtagField" layoutX="559.0" layoutY="52.0" promptText="hashtag" />
   </children>
</AnchorPane>
